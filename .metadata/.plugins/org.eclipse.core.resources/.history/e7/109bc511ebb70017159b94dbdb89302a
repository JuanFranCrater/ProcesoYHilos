package dam.psp;

class Contador{
	public static volatile int cuenta = 0;
	
}
class Sumador extends Thread {
		@Override
		public void run() {
			Contador.cuenta++;
		}
}
class REstador extends Thread {
	@Override
	public void run() {
		Contador.cuenta--;
	}
}
public class TestCondiciondeCarrera {

	public static void main(String[] args) {
		
		
	}

}
